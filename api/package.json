{
    "name": "drinxologist-api",
    "version": "0.1.0",
    "description": "API for drinxologist",
    "main": "index.js",
    "scripts": {
        "start": "npm run run-server",
        "start-local": "npm run build:local && npm run run-server",
        "build": "npm run build-server",
        "build-local": "npm run clean && concurrently \"npm run build-static-files:local\" \"npm run build-server\" && npm run move-static-files",
        "build-server": "tsc",
        "build-static-files:local": "cd ../frontend && npm run build && cd ../api",
        "move-static-files": "mv ../frontend/dist ./dist/public",
        "run-server": "node ./dist/index.js",
        "clean": "rimraf ./dist"
    },
    "repository": {
        "type": "git",
        "url": "git+ssh://git@github.com/relativeabsolute/drinxologist.git"
    },
    "keywords": [
        "express",
        "typescript",
        "cocktails",
        "mongodb",
        "mongo"
    ],
    "dependencies": {
        "cors": "^2.8.5",
        "dotenv": "^8.2.0",
        "express": "^4.17.1"
    },
    "devDependencies": {
        "@types/express": "^4.17.1",
        "@types/node": "^14.14.7",
        "tslint": "^6.1.3",
        "typescript": "^4.0.5",
        "rimraf": "^3.0.2"
    },
    "author": "Johan Burke",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/relativeabsolute/drinxologist/issues"
    },
    "homepage": "https://github.com/relativeabsolute/drinxologist#readme"
}
